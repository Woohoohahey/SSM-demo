<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dta/mybatis-3-mapper.dtd">
<mapper namespace="com.itactic.kaiy.ssm.dao.BookMapper">
    <insert id="addBook" parameterType="com.itactic.kaiy.ssm.entity.Book">
        insert into book(name,author) values (#{name},#{author})
    </insert>
    <select id="getAllBook" resultType="com.itactic.kaiy.ssm.entity.Book">
        select * from book
    </select>

</mapper>